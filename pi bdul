                <div id="ACEH" class="upage-content hidden vertical-col">
                    <div class="widget uib_w_53 scale-image d-margins" data-uib="media/img" data-ver="0">
                        <figure class="figure-align">
                            <img src="images/nangroe-aceh-darusalam .png">

